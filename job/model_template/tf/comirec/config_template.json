{
    "num_workers": 1,
    "node_affin": "only_cpu",
    "pod_affin": "spread",
    "timeout": "100d",
    "trainer": "runner",
    "job_detail": {
        "script_name": "/app/job/model_template/comirec/template.py",
        "model_args": {
            "name": "comirec_model"
        },
        "train_args": {
            "mw_com": "RING",
            "train_type": "compile_fit",
            "batch_size": 256,
            "epochs": 1,
            "optimizer": {
                "type": "adam",
                "args": {
                    "learning_rate": 0.001
                }
            },
            "losses": {
                "type": "sampled_softmax_loss",
                "args": {
                    "num_samples": 10
                }
            },
            "metrics": [
                [{
                    "type": "topk_hitrate",
                    "args": {
                        "k": 5,
                        "name": "top5_hitrate"
                    }
                },
                {
                    "type": "topk_hitrate",
                    "args": {
                        "k": 10,
                        "name": "top10_hitrate"
                    }
                },
                {
                    "type": "topk_hitrate",
                    "args": {
                        "k": 20,
                        "name": "top20_hitrate"
                    }
                }]
            ]
        }
    }
}